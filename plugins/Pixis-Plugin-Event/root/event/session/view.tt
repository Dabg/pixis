[% WRAPPER wrapper.tt %]
<h1>[% event.title %]</h1>
<h2>[% session.title %]</h2>

<table>
<tr>
<th>時間</th><td>[% session.start_on.strftime('%H:%M') %] - [% session.end_on.strftime('%H:%M') %]</th> 
</tr>
<tr>
<th>講師</th><td>[% session.presenter %] 氏</td>
</tr>
</table>
<p>[% session.description.replace("\n", "<br />") %]</p>
<p><a href="[% c.uri_for( "/event", event.id ) %]">&lt;&lt;&nbsp;イベントに戻る</a></p>
[% END %]
